{
   "compilerOptions": {
      "module": "CommonJS",
      "moduleResolution": "Node",
      "target": "ES2024",
      "lib": [
         "ES2024"
      ],
      "noEmit": true,
      "checkJs": true,
      "strict": true
   },
   "typeAcquisition": {
      "enable": true
   },
   "include": [
      "index.js",
      "src/**/*.js"
   ]
}